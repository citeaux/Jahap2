<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ContextMenu?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.jahap.gui.TextReportListController">
   <children>
      <BorderPane prefHeight="400.0" prefWidth="600.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <center>
            <TableView fx:id="reports" prefHeight="400.0" prefWidth="527.0" BorderPane.alignment="CENTER">
               <contextMenu>
                  <ContextMenu>
                    <items>
                      <MenuItem fx:id="setasStandard" mnemonicParsing="false" onAction="#setasStandard" text="%cxmenu.setasstandard" />
                        <MenuItem fx:id="copyreport" mnemonicParsing="false" onAction="#copyreport" text="%cxmenu.copyreport" />
                    </items>
                  </ContextMenu>
               </contextMenu></TableView>
         </center>
         <right>
            <AnchorPane prefHeight="400.0" prefWidth="76.0" BorderPane.alignment="CENTER">
               <children>
                  <Button fx:id="edit" mnemonicParsing="false" onAction="#edit" prefHeight="31.0" prefWidth="94.0" text="%button.edit" />
                  <Button fx:id="newbutton" layoutY="31.0" mnemonicParsing="false" onAction="#newbutton_pressed" prefHeight="31.0" prefWidth="94.0" text="%button.new" />
                  <Button fx:id="print" layoutY="62.0" mnemonicParsing="false" onAction="#print" prefHeight="31.0" prefWidth="94.0" text="%button.print" />
                  <Button fx:id="ok" layoutY="93.0" mnemonicParsing="false" onAction="#ok" prefHeight="31.0" prefWidth="94.0" text="%button.ok" />
                  <Button fx:id="cancel" layoutY="124.0" mnemonicParsing="false" onAction="#cancel" prefHeight="31.0" prefWidth="94.0" text="%button.cancel" />
               </children>
            </AnchorPane>
         </right>
      </BorderPane>
   </children>
</AnchorPane>
